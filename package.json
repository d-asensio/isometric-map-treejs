      "jest": true,
      "browser": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^6.4.0",
    "core-js": "3.8.1",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": ">=10.5.3",
    "mini-css-extract-plugin": "^1.3.2",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.0",
    "standard": "^16.0.3",
    "style-loader": "^2.0.0",
